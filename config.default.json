        },
        {
            "name": "vc-module-core",
            "type": "module",
            "url": "https://github.com/VirtoCommerce/vc-module-core"
        },
        {
            "name": "export",
            "type": "module",
            "url": "https://github.com/VirtoCommerce/vc-module-export"
        },
        {
            "name": "inventory",
            "type": "module",
            "url": "https://github.com/VirtoCommerce/vc-module-inventory"
        },
        {
            "name": "store",
            "type": "module",
            "url": "https://github.com/VirtoCommerce/vc-module-store"
        },
        {
            "name": "vc-module-catalog",
            "type": "module",
            "url": "https://github.com/VirtoCommerce/vc-module-catalog"
